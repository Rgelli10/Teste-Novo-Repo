package course;

import javax.swing.JOptionPane;

public class Program {

	public static void main(String[] args) {
		
		double n = Integer.parseInt(JOptionPane.showInputDialog("Enter a number: "));
	
		for(n = 0; n>0; n++) {
			
			JOptionPane.showMessageDialog(null, "Number is" + n);
		}
			
	
	}
	
	
}
