package application;

import javax.swing.JOptionPane;

public class Program9 {
	
	public static void main ( String [] args) {
		
		int n = Integer.parseInt(JOptionPane.showInputDialog("Insert a number peapole: "));
		double [] vect = new double [n];
		
		for (int i=0; i<n; i++) {
			vect[i] = n;
		}
	
	}
	
}
