public class Program7 {

	public static void main(String[] args) {
		

		CurrencyConverter.value =Integer.parseInt(JOptionPane.showInputDialog("How many dollars will be bought? "));
		
		double d = 0;
		
		CurrencyConverter.finalValue(d);
		
		JOptionPane.showMessageDialog(null, "Amount to be paid in reais = " + CurrencyConverter.finalValue(d));
		

		
	}

}
