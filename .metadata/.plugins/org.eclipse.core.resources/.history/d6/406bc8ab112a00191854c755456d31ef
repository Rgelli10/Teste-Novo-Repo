package course;

import javax.swing.JOptionPane;

public class Program {

	public static void main(String[] args) {
		
		JOptionPane.showInputDialog("How many integer numbers are you goint to enter? ");
		
		int n = Integer.parseInt(JOptionPane.showInputDialog("Enter a number: "));
		
		int sum =0;
	
		for(int i = 1; i<=n; i++) {
			
			
			JOptionPane.showMessageDialog(null, "value #" + i+ ": ");
			
			int x = Integer.parseInt(JOptionPane.showInputDialog(""));
			sum += x;
			
		}
			JOptionPane.showMessageDialog(null, "Sum = " + sum);
	
	}
	
	
}
