package entities;

public class Employee {

	public String name;
	public double grossSalary;
	public double tax;
	
	
	
	public double netSalary () {
		return grossSalary -= 1000;
	}
	
	public void increaseSalary(double p) {
		//p = ((tax/100)*netSalary()) + netSalary();
		p = netSalary() - 2;
		this.tax += p;
	}
	

}
